pipeline {
    agent any
    stages {
        stage('Clone') {
          steps {
            sh 'echo "Clone"'
          }
        }
        stage('Analyze') {
          steps {
            sh 'echo "Analyze"'
          }
        }
        stage('Build') {
            steps {
                sh 'echo "Hello World"'
                sh '''
                    echo "Multiline shell steps works too"
                    ls -lah
                '''
            }
        }
        stage('Test') {
          steps {
            sh 'echo "Test"'
          }
        }
        stage('Deploy') {
          steps {
            sh 'echo "Clone"'
          }
        }
    }
}
